<div class="row">
    <%= render "users/button-nav" %>
  </div>
  <h3>Uploads</h3>
<table>
  <% for upload in @user.uploads %>
    <tr>
      <td><%= upload.user_id %></td>
      <td><%= image_tag upload.photo.url(:medium), class: "img-thumbnail img-responsive" %></td>
      <td><%= link_to "Show", upload %></td>
      <td><%= link_to "Edit", edit_upload_path(upload) %></td>
      <td><%= link_to "Destroy", upload, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Upload", new_upload_path %></p>
